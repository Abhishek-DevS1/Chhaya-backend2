[Unit]
Description=Gunicorn for Chhaya

[Service]
User=abhishek
Type=simple
WorkingDirectory=/home/abhishek/CICDpro/Chhaya-backend
ExecStart=/home/abhishek/CICDpro/Chhaya-backend/venv/bin/gunicorn New_Backend.wsgi:application -b 0.0.0.0:8090 --error-logfile /var/log/geo_scanner/error.log --error-logfile /var/log/Chhaya/error.log --access-logfile /var/log/Chhaya/access.log
Restart=always
RestartSec=3

[Install]
